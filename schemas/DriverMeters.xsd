    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    >

    <xs:include schemaLocation="Meters.xsd"/>

    <xs:element name="DriverMeters">
        <xs:complexType>
            <xs:all>
                <xs:element name="Meters" type="MeterDefinitions"/>
            </xs:all>
        </xs:complexType>
    </xs:element>

</xs:schema>
